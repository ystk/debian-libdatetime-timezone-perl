+sub _last_observance { $last_observance }
+
+my $rules = [
+  bless( {
+    'at' => '0:00',
+    'from' => '2036',
+    'in' => 'Mar',
+    'letter' => 'D',
+    'name' => 'Iran',
+    'offset_from_std' => 3600,
+    'on' => '21',
+    'save' => '1:00',
+    'to' => 'max',
+    'type' => undef
